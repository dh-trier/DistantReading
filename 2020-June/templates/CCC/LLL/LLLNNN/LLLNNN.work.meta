<?xml version="1.0" encoding="UTF-8"?>
<object xmlns="http://textgrid.info/namespaces/metadata/core/2010" xmlns:tg="http://textgrid.info/relation-ns#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:tei="http://www.tei-c.org/ns/1.0">
  <generic>
    <provided>
      <title>#title#</title>
      <format>text/tg.work+xml</format>
    </provided>
  </generic>
  <work>
    <agent role="author" id="#xxx#">#author#</agent>
    <dateOfCreation notBefore="1840" notAfter="1840">1840</dateOfCreation>
    <!-- T1/T2/T3/T4 -->
    <temporal>
		<id type="/profileDesc/textClass/timeSlot@key">timeSlot</id>
		<value>#timeSlot#</value></temporal>
    <!-- female/male/mixed -->
    <subject>
		<id type="/profileDesc/textClass/authorGender@key">authorGender</id>
		<value>#authorGender#</value></subject>
	<!-- short/medium/long -->
	<subject>
		<id type="/profileDesc/textClass/size@key">size</id>
		<value>#size#</value></subject>
	<!-- low/high/unspecified -->
	<subject>
		<id type="/profileDesc/textClass/reprintCount@key">reprintCount</id>
		<value>#reprintCount#</value></subject>
	<genre>prose</genre>
  </work>
</object>
